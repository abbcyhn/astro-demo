---
import ArticleView from '../components/ArticleView.astro';
import Layout from '../layouts/Layout.astro';
import { siteConfig } from '../config';
import { getProcessedArticles } from '../utils/articles';

const { allArticles, latestArticles } = await getProcessedArticles();

const prevUrl = "";
const nextUrl = allArticles.length > siteConfig.articlesPerPage ? "/blog/2" : "";
---

<Layout title="Jeyhun Abbasov">
	<ArticleView prevUrl={prevUrl} nextUrl={nextUrl} articles={latestArticles}/>
</Layout>
