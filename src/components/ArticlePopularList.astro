---
import type  { CollectionEntry } from 'astro:content';
import ArticlePopular from './ArticlePopular.astro';

interface Props {
    articles: CollectionEntry<'articles'>[];
};

const { articles } = Astro.props;
---

<div class="space-y-4">
    {articles.map(article => <ArticlePopular article={article} />)}
</div>
